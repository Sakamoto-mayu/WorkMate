generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("VITE_APP_DATABASE_URL")
}

model Attendance {
  id       Int     @id @default(autoincrement())
  email    String
  month    String?
  date     String?
  day      String?
  status   String?
  clockIn  String?
  clockOut String?
  rest     String?
}
